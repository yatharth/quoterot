{
  "name": "quoterot-cdk",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "(cd infrastructure/lambdas/ && npm install);",
    "build": "tsc",
    "watch": "tsc --watch",
    "sanitycheck": "cdk synth",
    "local": "cdk synth --no-staging > template.yaml && sam local start-api",
    "deploy": "cdk bootstrap && cdk deploy",
    "destroy": "cdk destroy"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "^1.132.0",
    "@aws-cdk/aws-lambda": "^1.132.0",
    "@aws-cdk/aws-lambda-nodejs": "^1.132.0",
    "@aws-cdk/core": "^1.132.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.8",
    "esbuild": "^0.13.14",
    "typescript": "^4.5.2"
  }
}
