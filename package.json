{
  "name": "quoterot-cdk",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "sanitycheck": "npm run build && cdk synth",
    "diff": "npm run build && cdk diff",
    "local": "npm run build && cdk synth --no-staging > template.yaml && sam local start-api",
    "deploy": "npm run build && cdk bootstrap && cdk deploy --outputs-file ./cdk-outputs.json --require-approval never",
    "destroy": "cdk destroy"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "~1.134.0",
    "@aws-cdk/aws-cloudwatch-actions": "~1.134.0",
    "@aws-cdk/aws-dynamodb": "~1.134.0",
    "@aws-cdk/aws-events": "~1.134.0",
    "@aws-cdk/aws-events-targets": "~1.134.0",
    "@aws-cdk/aws-iam": "~1.134.0",
    "@aws-cdk/aws-lambda": "~1.134.0",
    "@aws-cdk/aws-lambda-destinations": "~1.134.0",
    "@aws-cdk/aws-lambda-event-sources": "~1.134.0",
    "@aws-cdk/aws-lambda-nodejs": "~1.134.0",
    "@aws-cdk/aws-sns": "~1.134.0",
    "@aws-cdk/aws-sns-subscriptions": "~1.134.0",
    "@aws-cdk/aws-sqs": "~1.134.0",
    "@aws-cdk/core": "~1.134.0",
    "app-root-path": "^3.0.0",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1032.0",
    "axios": "^0.24.0",
    "date-fns": "^2.27.0",
    "date-fns-tz": "^1.1.6",
    "p-iteration": "^1.1.8",
    "twitter-api-v2": "^1.6.5"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.85",
    "@types/app-root-path": "^1.2.4",
    "@types/node": "^16.11.8",
    "esbuild": "^0.13.14",
    "typescript": "^4.5.2"
  }
}
