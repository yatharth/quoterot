{
  "name": "quoterot-cdk",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "tsc && node make-env-file.js",
    "build:watch": "tsc --watch --preserveWatchOutput",
    "with-env": "env-cmd -x -f secrets/.env",
    "node": "npm run -- build && npm run -- with-env node",
    "cdk": "npm run -- build && npm run -- with-env cdk",
    "cdkw": "npm run -- build && npm run -- with-env npx cdkw",
    "cdk:sanitycheck": "npm run -- cdk synth",
    "cdk:diff": "npm run -- cdk diff",
    "cdk:local": "npm run -- cdk synth --no-staging > template.yaml && npm run -- with-env sam local start-api",
    "cdk:deploy": "npm run -- cdk deploy --outputs-file ./cdk-outputs.json --require-approval never",
    "cdk:destroy": "npm run -- cdk destroy",
    "lambdas:watch": "npm run -- cdkw \"**\"",
    "lambdas:logs": "npm run -- cdkw logs \"**\"",
    "queue:send": "sh src/archive-quoted-tweets/scripts/send-message-to-queue.sh",
    "twitter:get-user-id-by-username": "npm run -- node src/archive-quoted-tweets/scripts/get-user-id-by-username.js",
    "webhook:get": "npm run -- node src/reply-to-mentions/scripts/get-webhook.js",
    "webhook:remove": "npm run -- node src/reply-to-mentions/scripts/remove-webhook.js",
    "webhook:set": "npm run -- node src/reply-to-mentions/scripts/set-webhook.js",
    "webhook:dev": "npm run -- node src/reply-to-mentions/scripts/dev-webhook.js"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "~1.134.0",
    "@aws-cdk/aws-apigatewayv2": "~1.134.0",
    "@aws-cdk/aws-cloudwatch-actions": "~1.134.0",
    "@aws-cdk/aws-dynamodb": "~1.134.0",
    "@aws-cdk/aws-events": "~1.134.0",
    "@aws-cdk/aws-events-targets": "~1.134.0",
    "@aws-cdk/aws-iam": "~1.134.0",
    "@aws-cdk/aws-lambda": "~1.134.0",
    "@aws-cdk/aws-lambda-destinations": "~1.134.0",
    "@aws-cdk/aws-lambda-event-sources": "~1.134.0",
    "@aws-cdk/aws-lambda-nodejs": "~1.134.0",
    "@aws-cdk/aws-sns": "~1.134.0",
    "@aws-cdk/aws-sns-subscriptions": "~1.134.0",
    "@aws-cdk/aws-sqs": "~1.134.0",
    "@aws-cdk/aws-stepfunctions": "~1.134.0",
    "@aws-cdk/aws-stepfunctions-tasks": "~1.134.0",
    "@aws-cdk/core": "~1.134.0",
    "@types/debug": "^4.1.7",
    "@types/got": "^9.6.12",
    "@types/node-fetch": "^3.0.3",
    "@types/set-cookie-parser": "^2.4.2",
    "@types/url-join": "^4.0.1",
    "app-root-path": "^3.0.0",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1032.0",
    "cdk-watch": "^0.0.25",
    "date-fns": "^2.27.0",
    "date-fns-tz": "^1.1.6",
    "debug": "^4.3.3",
    "dotenv": "^10.0.0",
    "env-cmd": "^10.1.0",
    "glob": "^7.2.0",
    "got": "^11.8.3",
    "lodash": "^4.17.21",
    "node-fetch": "^3.1.0",
    "nodemon": "^2.0.15",
    "p-iteration": "^1.1.8",
    "qs": "^6.10.2",
    "runtypes": "^6.5.0",
    "set-cookie-parser": "^2.4.8",
    "twitter-api-v2": "^1.8.0",
    "twitter-autohook": "^1.7.2",
    "url-join": "^4.0.1",
    "uuid": "^8.3.2",
    "wayback-machine": "^0.2.1"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/aws-lambda": "^8.10.85",
    "@types/express": "^4.17.13",
    "@types/glob": "^7.2.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.11.8",
    "@types/tsc-watch": "^4.2.0",
    "@types/uuid": "^8.3.3",
    "esbuild": "^0.13.14",
    "express": "^4.17.2",
    "ngrok": "^4.2.2",
    "tsc-watch": "^4.6.0",
    "tw-activity": "^1.1.1",
    "typescript": "^4.5.2"
  }
}
